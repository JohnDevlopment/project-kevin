"language": "GDScript",
"path": "res://scripts/classes/nodes/AudioList.gd"
}, {
"base": "Resource",
"class": "AudioPack",
"language": "GDScript",
"path": "res://scripts/classes/resources/AudioPack.gd"
}, {
"base": "Resource",
"class": "ErrorInfo",
"language": "GDScript",
"path": "res://scripts/classes/resources/ErrorInfo.gd"
}, {
"base": "Area2D",
"class": "GameObject",
"language": "GDScript",
"path": "res://scripts/classes/nodes/GameObject.gd"
}, {
