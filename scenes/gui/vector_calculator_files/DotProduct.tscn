script/source = "extends VBoxContainer

var vector_a: VarRef
var vector_b: VarRef

signal vector_calc_result(value)

func _return_value():
	var a = $Inputs/VectorA.get_value()
	var b = $Inputs/VectorB.get_value()
	
	print(a, \" \", b)
	
	vector_a.set_object(a)
	vector_b.set_object(b)
	
	emit_signal(\"vector_calc_result\", a.dot(b))

func _connect(node: Node, method: String) -> void:
	var temp = [vector_a, vector_b]
	connect(\"vector_calc_result\", node, method, [\"dot\", temp], CONNECT_DEFERRED)

func _ready():
	vector_a = VarRef.new(Vector2())
	vector_b = VarRef.new(Vector2())
"

